<?xml version="1.0" encoding="utf-8"?>
<plugin id="github">
  <protocol-implementation id="github" protocol="git">
    <pull-server>
      <filename>plugin.py</filename>
      <classname>GithubPullServer</classname>
    </pull-server>
    <push-server>
      <filename>plugin.py</filename>
      <classname>GitPushServer</classname>
    </push-server>
  </protocol-implementation>




  <data-provider id="github">
    <filename>plugin.py</filename>
    <classname>DataProvider</classname>
    <roles>
        <source/>
        <sink/>
        <twoway/>
    </roles>
    <supported-protocols>
        <protocol>fpemud-refsystem-protocol</protocol>
    </supported-protocols>
    <match-peer>
        <my-peer-id/>
        <auto/>
    </match-peer>
  </data-provider>

</plugin>
