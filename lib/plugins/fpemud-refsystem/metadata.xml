<?xml version="1.0" encoding="utf-8"?>
<plugin id="fpemud-refsystem">
    <data-type id="fpemud-refystem-private-data">
        <name>fpemud's private system data</name>
    </data-type>
    <protocol id="fpemud-refsystem-private-data-sync">
        <name>fpemud's private system data synchronization protocol</name>
        <capabilities>
            <get/>
        </capabilities>
    </protocol>
    <data-provider id="fpemud-refsystem-private-data">
        <filename>plugin.py</filename>
        <classname>DataProviderPrivateData</classname>
        <roles>
            <source/>
            <sink/>
        </roles>
        <data-types>
            <data-type id="fpemud-refystem-private-data"/>
        </data-types>
        <protocols>
            <protocol id="fpemud-refsystem-private-data-sync"/>
        </protocols>
    </data-provider>
</plugin>
